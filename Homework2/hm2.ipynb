    "def preorder(tree, u):\n",
    "\n",
    "    if u not in tree:\n",
    "        return [u]\n",
    "\n",
    "    A = [u]\n",
    "    for v in tree[u] :\n",
    "        if v != u :\n",
    "            A = A + preorder(tree, v)\n",
    "\n",
    "    return A"
   ],
