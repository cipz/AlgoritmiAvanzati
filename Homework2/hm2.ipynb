    "def mst_to_tree(parents):\n",
    "\n",
    "    if len(parents) == 0:\n",
    "        return None\n",
    "\n",
    "    if len(parents) == 1:\n",
    "        nodo, padre = parents.popitem()\n",
    "        return {padre: [nodo]}\n",
    "\n",
    "    nodo, padre = parents.popitem()\n",
    "    tree = mst_to_tree(parents)\n",
    "\n",
    "    if padre in tree:\n",
    "        tree[padre].append(nodo)\n",
    "    else:\n",
    "        tree[padre] = [nodo]\n",
    "\n",
    "    return tree\n"
   ],
