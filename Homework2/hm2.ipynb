    "def ham_circ_weight(ham_circ):\n",
    "\n",
    "    total_weight = 0\n",
    "    for j in range(1, len(ham_circ)):\n",
    "        i = j - 1\n",
    "        total_weight += weight(vertexes[ham_circ[i]][1], vertexes[ham_circ[j]][1])\n",
    "\n",
    "    return total_weight"
